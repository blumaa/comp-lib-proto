{
  "name": "@comp-lib-proto/tokens",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:tokens && yarn build:ts",
    "build:tokens": "node --loader ts-node/esm src/scripts/build.ts",
    "build:ts": "tsc",
    "dev": "nodemon --watch src/tokens -e json --exec 'yarn build:tokens'"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
